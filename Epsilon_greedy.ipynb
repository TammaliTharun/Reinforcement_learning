   "source": [
    "class EXP3():\n",
    "    def __init__(self,d,w,rewards,eta,gamma):\n",
    "        self.d = d\n",
    "        self.w = w\n",
    "        self.probabilities=[]\n",
    "        self.cum_reward=0\n",
    "        self.best_cum_reward = 0\n",
    "        self.total_regret = 0\n",
    "        self.rewards_hat = []\n",
    "        self.regrets = []\n",
    "        self.rewards_T = rewards\n",
    "        self.cum_reward_vector = np.zeros(d)\n",
    "        self.cum_regrets = np.zeros(T)\n",
    "        self.arm_choosen_count = np.zeros(d)\n",
    "        self.gamma = gamma\n",
    "        self.eta = eta\n",
    "\n",
    "\n",
    "    def update_mean(self,chosen_arm,loss):\n",
    "        self.arm_choosen_count[chosen_arm]+=1\n",
    "        self.cum_loss_vector[chosen_arm] = ((self.cum_loss_vector[chosen_arm]*(self.arm_choosen_count[chosen_arm]-1))+loss)/self.arm_choosen_count[chosen_arm]\n",
    "        \n",
    "        \n",
    "    def run(self,T):\n",
    "        for t in range(0,T):\n",
    "            self.rewards = self.rewards_T[t] \n",
    "            r_hat = np.zeros(d)\n",
    "    \n",
    "            probability = self.w/np.sum(self.w)\n",
    "            self.probabilities.append(probability)\n",
    "            \n",
    "            if np.isnan(probability).any():\n",
    "                probability = np.nan_to_num(probability, nan=1e-10)\n",
    "                \n",
    "            action = np.random.choice(d,p=self.probabilities[t])#choose an arm based on probability distribution\n",
    "            curr_reward = self.rewards[action]\n",
    "            self.cum_reward+= curr_reward\n",
    "\n",
    "            best_reward_arm = np.argmax(self.rewards)\n",
    "            best_reward = self.rewards[best_reward_arm]\n",
    "            self.best_cum_reward+=best_reward\n",
    "\n",
    "            \n",
    "            # Regret calculation\n",
    "            regret = self.best_cum_reward - self.cum_reward \n",
    "            self.total_regret+= regret\n",
    "            self.cum_regrets[t] = regret\n",
    "            \n",
    "            r_hat[action] = curr_reward/self.probabilities[t][action]\n",
    "            self.w[action] = self.w[action]*np.exp(-self.eta*r_hat[action])\n",
    "                        \n",
    "        return self.cum_regrets\n",
    "\n",
    "        \n",
    "        "
   ]
