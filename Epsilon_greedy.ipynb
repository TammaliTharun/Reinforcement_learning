   "source": [
    "def beta_distribuion(d,alpha,beta):\n",
    "    beta_numbers_reward = []\n",
    "    beta_numbers_loss = []    \n",
    "    for i in range(1,d+1):\n",
    "        n = np.random.beta(alpha, beta)\n",
    "        beta_numbers_reward.append(n)\n",
    "        beta_numbers_loss.append(1-n)\n",
    "    return beta_numbers_reward, beta_numbers_loss\n",
    "        \n",
    "def generate_reward(alpha,beta,T,d):\n",
    "    reward_vector = []\n",
    "    for i in range(0,T):\n",
    "        reward_vector.append([np.round(np.random.beta(alpha,beta*i),2) for i in range(1,d+1)])\n",
    "    return reward_vector\n",
    "\n",
    "\n",
    "class epsilon_greedy():\n",
    "    def __init__(self,d,rewards,epsilon,T):\n",
    "        cum_reward=0\n",
    "        cum_loss = 0\n",
    "        self.rewards_T = rewards\n",
    "        self.regrets = []\n",
    "        self.cum_loss_vector = np.zeros(d)\n",
    "        self.cum_reward_vector = np.zeros(d)\n",
    "        self.arm_choosen_count = np.zeros(d)  \n",
    "        self.cum_regret = np.zeros(T)\n",
    "        self.cum_regret_new = np.zeros(T)\n",
    "        self.total_regrets = np.zeros(T)\n",
    "        self.epsilon = epsilon\n",
    "        self.best_reward = 0\n",
    "        self.cum_reward = 0\n",
    "        self.best_cum_reward = 0\n",
    "        self.total_regret = 0\n",
    "        self.cum_loss = 0\n",
    "        self.d = d\n",
    "        \n",
    "    def select_action(self,t,rewards):\n",
    "        toss = random.random()\n",
    "        if(toss<self.epsilon):\n",
    "            chosen_arm = random.choice(range(self.d))\n",
    "        else:\n",
    "            chosen_arm = np.argmax(rewards)\n",
    "        return chosen_arm\n",
    "    \n",
    "    def update_loss(self,regret,t):\n",
    "        regret_new = []\n",
    "        for a in range(0,self.d):\n",
    "            regret_new.append(regret*(self.arm_choosen_count[a]/t))\n",
    "        return regret_new\n",
    "        \n",
    "    def run(self,T):\n",
    "        for t in range(0,T):\n",
    "            self.rewards = self.rewards_T[t]\n",
    "            # print(self.rewards)\n",
    "            chosen_arm = self.select_action(t,self.rewards)\n",
    "            curr_reward = self.rewards[chosen_arm]\n",
    "            self.cum_reward+= curr_reward\n",
    "\n",
    "            best_reward_arm = np.argmax(self.rewards)\n",
    "            best_reward = self.rewards[best_reward_arm]\n",
    "            self.best_cum_reward+=best_reward\n",
    "            \n",
    "            regret = self.best_cum_reward - self.cum_reward \n",
    "            self.total_regret+= regret\n",
    "            self.cum_regret[t] = regret\n",
    "            # print(self.best_cum_reward,self.cum_reward,regret)\n",
    "\n",
    "        return self.cum_regret\n",
    "\n",
    "\n"
   ]
